INCLUDE "prelude.rby".

pool2d w = mfork 4; 
           [
             D^(w+1), D^w,
             D,       id
           ];
           group 2 2;
           [max, max];
           max;
           bundle (2*w);
           half w;
           pi2;
           group (w/2) 2;
           map (w/2) pi2;
           inv_bundle (w/2)
           .

current = pool2d 4 .
